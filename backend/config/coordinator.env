# Application Configuration
APP_MODE=coordinator
API_PORT=8000
HONEYPOT_PORT=9000

# Database
DB_PATH=/app/data/knowledge_base.db

# Federated Learning
FEDERATED_ROUNDS=10
LOCAL_EPOCHS=3
BATCH_SIZE=32
LEARNING_RATE=0.01
MIN_ORGANIZATIONS=2

# Differential Privacy
DP_EPSILON=0.5
DP_DELTA=1e-6
DP_METHOD=laplacian
CLIPPING_THRESHOLD=1.0

# AWS (Optional)
AWS_REGION=us-east-1
S3_BUCKET=sqli-models


